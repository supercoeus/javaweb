@CHARSET "UTF-8";
.info-imgs li {
    border: 1px solid #abadb3;
    float: left;
    height: 180px;
    left: 0;
    margin: 0 10px 15px 0;
    position: relative;
    top: 0;
    width: 190px;
    z-index: 1;
}
 .info-imgs li img {
    height: 180px;
    width: 190px;
}
.info-imgs li.last {
    cursor: pointer;
    font-size: 40px;
    line-height: 80px;
    position: relative;
    text-align: center;
}
 .info-imgs li.last input {
    border: medium none;
    cursor: pointer;
    font-size: 100px;
    height: 200px;
    left: 0;
    opacity: 0;
    position: absolute;
    top: 0px;
    width:210px;
}
 .info-imgs li i {
    background: #999 none repeat scroll 0 0;
    border-radius: 10px;
    color: #fff;
    cursor: pointer;
    display: none;
    font-size: 18px;
    height: 20px;
    line-height: 20px;
    position: absolute;
    right: -10px;
    text-align: center;
    top: -10px;
    width: 20px;
    z-index: 10;
}
