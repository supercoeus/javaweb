package com.lsgx.admin.model;

import java.io.Serializable;
import java.util.Date;

public class Company implements Serializable
{
    
    /**
     * 
     */
    private static final long serialVersionUID = 8424620803657503314L;
    
    /**
     * 企业
     */
    
    private Integer id;
    
    /**
     * 企业名称
     */
    private String name;
    
    private String address;
    
    /**
     * 负责人
     */
    private String head;
    
    private String mobile;
    
    /**
     * 企业简介
     */
    private String introduction;
    
    /**
     * 余额
     */
    private Double balance;
    
    /**
     * 企业注册号
     * 
     */
    private String registerNo;
    
    /**
     * 组织机构代码
     */
    private String groupCode;
    
    /**
     * 法定代表人姓名
     */
    private String legalName;
    
    /**
     * 身份证
     */
    private String identity;
    
    /**
     * 企业认证图片,多个用逗号分隔
     */
    private String contentImageId;
    
    /**
     * 手机验证状态
     */
    private Integer mobileState;
    
    /**
     * 头像
     */
    private Integer coverImageId;
    
    /**
     * 公司网址
     */
    private String url;
    
    /**
     * 企业类型
     */
    private Integer type;
    
    /**
     * 企业Logo
     */
    private Integer conpanyImageId;
    
    /**
     * 企业信息
     */
    private String infomation;
    
    /**
     * 企业使用状态
     */
    private Integer useState;
    
    /**
     * 企业认证状态
     */
    private Integer state;
    
    /**
     * 发起人
     */
    private Integer userId;
    
    private Date registerTime;
    
    private Date lastUpdateTime;
    
    /**
     * 业务范围
     */
    private String serviceField;
    
    private String serviceFieldShort;
    
    /**
     * 助善口令
     */
    private String goodPassWord;

    public Integer getId()
    {
        return id;
    }
    
    public String getServiceField()
    {
        return serviceField;
    }
    
    public void setServiceField(String serviceField)
    {
        this.serviceField = serviceField;
    }

    public Date getRegisterTime()
    {
        return registerTime;
    }
    
    public void setRegisterTime(Date registerTime)
    {
        this.registerTime = registerTime;
    }
    
    public Date getLastUpdateTime()
    {
        return lastUpdateTime;
    }
    
    public void setLastUpdateTime(Date lastUpdateTime)
    {
        this.lastUpdateTime = lastUpdateTime;
    }
    
    public void setId(Integer id)
    {
        this.id = id;
    }
    
    public String getName()
    {
        return name;
    }
    
    public void setName(String name)
    {
        this.name = name;
    }
    
    public String getAddress()
    {
        return address;
    }
    
    public void setAddress(String address)
    {
        this.address = address;
    }
    
    public String getHead()
    {
        return head;
    }
    
    public void setHead(String head)
    {
        this.head = head;
    }
    
    public String getMobile()
    {
        return mobile;
    }
    
    public void setMobile(String mobile)
    {
        this.mobile = mobile;
    }
    
    public String getIntroduction()
    {
        return introduction;
    }
    
    public void setIntroduction(String introduction)
    {
        this.introduction = introduction;
    }
    
    public Double getBalance()
    {
        return balance;
    }
    
    public void setBalance(Double balance)
    {
        this.balance = balance;
    }
    
    public String getRegisterNo()
    {
        return registerNo;
    }
    
    public void setRegisterNo(String registerNo)
    {
        this.registerNo = registerNo;
    }
    
    public String getGroupCode()
    {
        return groupCode;
    }
    
    public void setGroupCode(String groupCode)
    {
        this.groupCode = groupCode;
    }
    
    public String getLegalName()
    {
        return legalName;
    }
    
    public void setLegalName(String legalName)
    {
        this.legalName = legalName;
    }
    
    public String getIdentity()
    {
        return identity;
    }
    
    public void setIdentity(String identity)
    {
        this.identity = identity;
    }
    
    public String getContentImageId()
    {
        return contentImageId;
    }
    
    public void setContentImageId(String contentImageId)
    {
        this.contentImageId = contentImageId;
    }
    
    public Integer getMobileState()
    {
        return mobileState;
    }
    
    public void setMobileState(Integer mobileState)
    {
        this.mobileState = mobileState;
    }
    
    public Integer getCoverImageId()
    {
        return coverImageId;
    }
    
    public void setCoverImageId(Integer coverImageId)
    {
        this.coverImageId = coverImageId;
    }
    
    public String getUrl()
    {
        return url;
    }
    
    public void setUrl(String url)
    {
        this.url = url;
    }
    
    public Integer getType()
    {
        return type;
    }
    
    public void setType(Integer type)
    {
        this.type = type;
    }
    
    public Integer getConpanyImageId()
    {
        return conpanyImageId;
    }
    
    public void setConpanyImageId(Integer conpanyImageId)
    {
        this.conpanyImageId = conpanyImageId;
    }
    
    public String getInfomation()
    {
        return infomation;
    }
    
    public void setInfomation(String infomation)
    {
        this.infomation = infomation;
    }
    
    public Integer getUseState()
    {
        return useState;
    }
    
    public void setUseState(Integer useState)
    {
        this.useState = useState;
    }
    
    public Integer getState()
    {
        return state;
    }
    
    public void setState(Integer state)
    {
        this.state = state;
    }
    
    public Integer getUserId()
    {
        return userId;
    }
    
    public void setUserId(Integer userId)
    {
        this.userId = userId;
    }
    
    @Override
    public String toString()
    {
        return "Company [id=" + id + ", name=" + name + ", address=" + address + ", head=" + head + ", mobile=" + mobile + ", introduction=" + introduction + ", balance=" + balance + "]";
    }
    
    public String getServiceFieldShort()
    {
        return serviceFieldShort;
    }
    
    public void setServiceFieldShort(String serviceFieldShort)
    {
        this.serviceFieldShort = serviceFieldShort;
    }
    
    public void setGoodPassWord(String goodPassWord){
    	this.goodPassWord = goodPassWord;
    }
    
    public String getGoodPassWord(){
    	return goodPassWord;
    }
    
}
